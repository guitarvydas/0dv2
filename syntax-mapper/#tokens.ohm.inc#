    dqb = "“"
    dqe = "”"
  stringRaw = dqb (~dqb ~dqe any)* dqe
  string = stringRaw ws

    sqb = "❛"
    sqe = "❜"
  charconstRaw = sqb (~sqb ~sqe any)* sqe
  charconst = charconstRaw ws

  idRaw = "❲" (~"❲" ~"❳" any)* "❳"
  id = idRaw ws


    intb = "⟨"
    inte = "⟩"
  integerRaw = intb (~intb ~inte any)* inte
  integer = integerRaw ws

    symb = "❲"
    syme = "❳"
  symbolRaw = symb (~symb ~syme any)* syme
  symbol = symbolRaw ws
  symRaw<text> = symb text syme
  sym<text> = symRaw<text> ws
    
  //   kwb = "‹"
  //   kwe = "›"
  // keyword = kwb (~kwb ~kwe any)* kwe

    mcb = "⟪"
    mce = "⟫"
  multi_char_operatorRaw = mcb (~mcb ~mce any)* mce
  multi_char_operator = multi_char_operatorRaw ws

  indentc = "⇢"
  dedentc = "⇠"

    vb = "«"
    ve = "»"
  verbatim = vb (~vb ~ve any)* ve

  comment = "⎣" (~"⎣" ~"⎦" any)* "⎦"

  anyToken1 = string | charconst | id | integer | keyword | multi_char_operator | indentc | dedentc | verbatim | comment | symbol | integer | any
  anyToken = anyToken1

    uspc = "・"
    unl = "⦚"
    in_nl = unl
    
    spc = " "
    nl = "\n"

    commentspace = "◦"
    commentnl = "¶"

    stringspace = "□"
    stringnl = "‡"
    charspace = "☐"
    charnl = "†"

    wsc = space | spc | nl | uspc | unl | indentc | dedentc | commentspace | commentnl | stringspace | stringnl | charspace | charnl | verbatim
    ws = wsc*
