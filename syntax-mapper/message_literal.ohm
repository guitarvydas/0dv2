Message_Literal_RewriteForCL {
  program = item+
  item =
    | applySyntactic<Message_Literal> -- append
    | any -- other

  Message_Literal = "Message" "(" id ")" "{" Operand "," Operand "}"

  Operand =
    | Operand "." id -- recursive
    | id -- bottom

  id = idfirst idrest*
  idfirst = letter | "_"
  idrest = alnum | "_"
  comma = ","
  string = dq (~dq any)* dq
  dq = "\""
  uspc = "・"
  unl = "⦚"
  space += uspc | unl
}
